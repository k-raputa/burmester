{
    "name": "udg-shopware\/swag-customized-products",
    "description": "Custom Products for Shopware 6 ",
    "version": "2.6.1",
    "type": "shopware-platform-plugin",
    "license": "proprietary",
    "authors": [
        {
            "name": "Shopware"
        }
    ],
    "require": {
        "shopware\/core": "~6.3.0",
        "shopware\/storefront": "~6.3.0"
    },
    "extra": {
        "shopware-plugin-class": "Swag\\CustomizedProducts\\SwagCustomizedProducts",
        "copyright": "(c) by shopware AG",
        "label": {
            "de-DE": "Custom Products",
            "en-GB": "Custom Products"
        },
        "description": {
            "de-DE": "Fulfill your customers desire for customizable and personalized products with Custom Products.",
            "en-GB": "Erf\u00fclle mit Custom Products Deinen Kunden den Wunsch nach individualisierbaren und personalisierten Produkten."
        },
        "manufacturerLink": {
            "de-DE": "https:\/\/store.shopware.com\/shopware-ag.html",
            "en-GB": "https:\/\/store.shopware.com\/en\/shopware-ag.html"
        },
        "supportLink": {
            "de-DE": "https:\/\/issues.shopware.com\/",
            "en-GB": "https:\/\/issues.shopware.com\/"
        }
    },
    "autoload": {
        "psr-4": {
            "Swag\\CustomizedProducts\\": "src\/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swag\\CustomizedProducts\\Test\\": "tests\/"
        }
    },
    "suggest": {
        "shopware\/administration": "Require '*'. Allows you to to set up the plugin easily",
        "shopware\/storefront": "Require '*'. Enables special features like configuring Custom Products on the product detail page."
    },
    "conflict": {
        "shopware\/storefront": "<6,>=7",
        "shopware\/administration": "<6,>=7"
    }
}
